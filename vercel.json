{
  "buildCommand": "SKIP_ENV_VALIDATION=true SKIP_LINT=true npm run build:no-lint",
  "ignoreCommand": "exit 0",
  "outputDirectory": ".next",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "SKIP_ENV_VALIDATION": "true",
    "SKIP_LINT": "true"
  }
}